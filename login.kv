#:include signup.kv

<ClearButton@Button>:
    background_color: (0,0,0,0)
    background_normal: ''
    back_color: (0,0,1, 0.0)
    border_radius:  [18]
    canvas.before:
        Color:
            rgba: self.back_color
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: self.border_radius
<LogIn>:
    user: user
    password: password
    info:info
    BoxLayout:
        id: login_layout
        orientation: 'vertical'
        padding: [0.2*(root.width),0.18*(root.height), 0.2*(root.width),  0]
        BoxLayout:
            size_hint_y: 0.2
            canvas.before:
                Color:
                    rgba: color1dark
                Triangle:
                    points: [root.width, root.height, 0, root.height, root.width, root.height*0.7]
                Color:
                    rgba: color1
                Triangle:
                    points: [0, root.height, root.width, root.height, 0, root.height*0.7]
            Label:
                text: '[b]Nutribase [/b]'
                markup: True
                color: 61/255, 55/255, 134/255
                font_size: '30sp'
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: 0.2
            Label:
                id: info
                text: ''
                markup: True
                size_hint_y: 0.05
                color: 'black'
                text_size: root.width-20, None
                padding_x: 0.2*root.width
            BoxLayout:
            	size_hint_y: 0.45
                MDTextField:
                    id: user
                    hint_text: "User Name"
                    write_tab: False
                    multiline:False
                    line_color_focus: color1
                    size_hint_y: None
                    size_hint_x: 0.7
                    pos_hint: {"x":0.25, "y":0.1}
                    color: 'black'

            BoxLayout:
            	size_hint_y: 0.45
                MDTextField:
                    id: password
                    hint_text: "Password"
                    write_tab: False
                    multiline:False
                    on_text_validate: root.do_login()
                    password:True
                    line_color_focus: color1
                    size_hint_y: None
                    size_hint_x: 0.7
                    pos_hint: {"x":0.25, "y":0.1}
                    color: 'black'

        BoxLayout:
            orientation: "horizontal"
            size_hint_y: 0.25
            spacing:15
            AnchorLayout:
                anchor_x:'right'
                anchor_y:'center'
                LineButtonMD:
                    text: '[b]Sign Up[/b]'
                    on_press: root.do_signup()
            AnchorLayout:
                anchor_x:'left'
                anchor_y:'center'
                FilledButtonMD:
                    text: '[b]Log In[/b]'
                    on_press: root.do_login()
        AnchorLayout:
            size_hint_y: 0.1
            ClearButton:
                text: 'Forgot password'
                bold: True
                size_hint_x: 0.3
                color: 'black'
                on_press: root.forgotpassword()

        BoxLayout:
            size_hint_y:0.25 - (0.18*(root.height))/root.height


<SelectableLabel>:
    text_size: self.size
    id_label1: ''
    id_label2: ''
    background_normal: ''
    back_color: color255
    border_radius:  25
    color: self.back_color
    bold: True
    canvas.before:
        Color:
            rgba: color255dark  if self.selected else color255light
        Line:
            width: 1.8
            rounded_rectangle: (self.pos[0], self.pos[1], self.size[0], self.size[1], self.border_radius)

    BoxLayout:
        orientation: "horizontal"
        BoxLayout:
            padding:[0, 0, 0,  0 ]
            size_hint_x: 0.1
            Button:
                background_color: (0, 0, 255, 0)
                on_press:root.patientpopup()
                Image:
                    source:'images/trashicon.png'
                    center_x: self.parent.center_x-5
                    center_y: self.parent.center_y-10
                    size_hint: None, None
                    size: (38,38)
        LargeLabel:
            id: id_label1
            text: root.id_label1
            size_hint_x: 0.4
            halign:'center'
            valign:'middle'
        BoxLayout:
            size_hint_x: 0.001
            background_normal: ''
            back_color: color255
            border_radius:  0
            color: self.back_color
            bold: True
            canvas.before:
                Color:
                    rgba: color255light
                Line:
                    width: 1.0
                    rounded_rectangle: (self.pos[0], self.pos[1], self.size[0], self.size[1], self.border_radius)
            Label:
                id: name_screen
                text: ""
                color: 'black'
        LargeLabel:
            id: id_label2
            text: root.id_label2
            size_hint_x: 0.3
            valign:'middle'
            halign:'center'
        BoxLayout:
            size_hint_x :0.2
            orientation:"horizontal"
            padding: (10, 5, 0, 5)
            Button:
                id: buttonrv
                text: "[b]Expand[/b]"
                font_size: small if root.width> 1500 else vsmall
                markup: True
                background_color: 0, 0, 1, 0.03
                color:'black'
                manager: None
                name_screen: ''
                on_press: root.printed()
                on_release:
                    self.manager.current = self.name_screen

<RV>:
    viewclass: 'SelectableLabel'
    SelectableRecycleBoxLayout:
        default_size: None, dp(42)
        default_size_hint: 1, None
        size_hint_y: None
        height: self.minimum_height
        orientation: 'vertical'
        multiselect: False
        touch_multiselect: False
        padding:(0.01*root.width,5,0.01*root.width,5)
        spacing: 4

<Connected>:
    id: connected
    BoxLayout:
        orientation: "vertical"
        patientlist: patientlist
        filterpic: filterpic
        filterwards: filterwards
        BoxLayout:
            size_hint_y: 0.08
            ActionBar:
                pos_hint: {'top':1}
                background_color: color1ForTop
                app_icon: "images/kivyicon.png"
                ActionView:
                    ActionPrevious:
                        previous_image: "images/chevron-left.png"
                        title: "[b]NB [/b]"
                        markup:True
                        color: 'white'
                        with_previous: True
                        on_press: root.back()
                    ActionButton:
                        text: "[b]Log out[/b]"
                        markup: True
                        color: "white"
                        font_size: big if root.width> 1500 else small
                        on_press: root.disconnect()
                    ActionOverflow:
                        overflow_image: "images/dots-vertical.png"
                        ActionButton:
                            text: "[b]Profile[/b]"
                            markup: True
                            color:"white"
                            background_color: color1
                            font_size: big if root.width> 1500 else small
                            on_press: root.profile()
                        ActionButton:
                            text: "[b]About[/b]"
                            markup: True
                            background_color: color1
                            font_size: big if root.width> 1500 else small
                            on_press: root.about()
                        ActionButton:
                            text: "[b]Delete User[/b]"
                            markup: True
                            color: "white"
                            background_color: color1
                            font_size: big if root.width> 1500 else small
                            on_press: root.deleting_user_popup()
        BoxLayout:
            orientation: "vertical"
            spacing: 0
            size_hint_y: 0.92
            BoxLayout:
                size_hint_y: 0.08
                padding:(0.01*root.width,10,0,0)
                BoxLayout:
                    padding:(10,0,10,0)
                    spacing: root.width*0.02 if root.width*0.02>40 else 40
                    BoxLayout:
                        size_hint_x: 0.55 -12/root.width
                        pos_hint_x: 0.1
                        MDTextField:
                            id: patient_number
                            line_color_focus: color1
                            pos_hint: {"x":0.25, "y":-0.1}
                            font_size: big
                            hint_text: "Search by Patient Number"
                            write_tab: False
                            multiline:False
                            on_text: root.search_hospital_patient_text(self.text)
                            on_text_validate: root.search_hospital_patient()
                    BoxLayout:
                        size_hint_x: 0.35
                        spacing: 6

                        FilledButtonMD:
                            text: f"[b]New Patient[/b]"
                            on_press: root.new_hospital_patient()

                    BoxLayout:
                        size_hint_x:0.1
            BoxLayout:
                size_hint_y: 0.05
                orientation:"horizontal"
                BoxLayout:
                    size_hint_x: 0.05
                BoxLayout:
                    size_hint_x: 0.1
                    Button:
                        id: filterwards
                        background_color: (0, 0 , 1, 0)
                        on_press: root.filtered()
                        Image:
                            id:filterpic
                            source:'images/filteroff.png'
                            x: self.parent.x-5
                            center_y: self.parent.center_y-10
                            size_hint: None, None
                            size: (48,48)
                BoxLayout:
                    padding: (0,0,0,0)
                    orientation:"horizontal"
                    size_hint_x: 0.85
                    LargeLabel:
                        size_hint_x: 0.5
                        text: "Patient Name"
                        italic: True
                    LargeLabel:
                        size_hint_x: 0.4
                        text: "Patient Number"
                        italic: True
                    Label:
                        size_hint_x: 0.1
            BoxLayout:
                size_hint_y: 0.87
                padding:(0.01*root.width,5,0.01*root.width,5)
                RV:
                    id: patientlist
                    padding:(0.01*root.width,5,0.01*root.width,5)
                    rows1: [("It seems like you are the first staff from your hospital to use this app", "If this is wrong please check on your hospital name spelling")]
                    name : connected
                    data: [{'id_label1': str(x[0]), 'id_label2': str(x[1]), "manager": root.manager, "name_screen": self.name} for x in self.rows1]

<About>:
    BoxLayout:
        orientation: "vertical"
        BoxLayout:
            size_hint_y: 0.03
            ActionBar:
                pos_hint: {'top':1}
                background_color: color1ForTop
                ActionView:
                    ActionPrevious:
                        previous_image: "images/chevron-left.png"
                        title: "[b]NB [/b]"
                        markup:True
                        color: "white"
                        with_previous: True
                        on_press: root.back()
                    ActionButton:
                        text: "[b]Log out[/b]"
                        markup: True
                        font_size: big if root.width> 1500 else small
                        color: "white"
                        on_press: root.disconnect()
                    ActionOverflow:
                        overflow_image: "images/dots-vertical.png"
                        ActionButton:
                            text: "[b]Profile[/b]"
                            markup: True
                            font_size: big if root.width> 1500 else small
                            background_color: color1
                            on_press: root.profile()
        Label:
            size_hint_y: 0.04
        Accordion:
            orientation:"vertical"
            size_hint_y: 0.93

            AccordionItem:
                id: a1
                title: "Where is the information on the app from?"
                collapse: False
                background_normal: 'images/onpress.png'
                background_selected: 'images/pressed.png'
                color: 'black'
                BoxLayout:
                    orientation: "vertical"
                    spacing: 10
                    Label:
                        color: "black"
                        size: self.texture_size
                        text_size: root.width-20, None
                        text: "Nestlé Nutrition Institute - MNA® Elderly - Overview. (2021). Retrieved from https://www.mna-elderly.com/"
                    Label:
                        color: "black"
                        size: self.texture_size
                        text_size: root.width-20, None
                        text: "Micronutrient Exam: Clinical Interpretation of Nutrition Focused Physical Exam Findings (n.d.). Retrieved from https://www.todaysdietitian.com/newarchives/images/MicronutrientTable0318.pdf"
                    Label:
                        color: "black"
                        size: self.texture_size
                        text_size: root.width-20, None
                        text: "Strickland, A. (2017, April 13). Nutrition Focused Physical Exam (NFPE). Indiana Academy of Nutrition and Dietetics Annual Conference"

            AccordionItem:
                id: a2
                title: "The app crashed unexpectedly"
                collapse: True
                background_normal: 'images/onpress.png'
                background_selected: 'images/pressed.png'
                BoxLayout:
                    orientation: "vertical"
                    padding_y: 15
                    Label:
                        color: "black"
                        text_size: root.width-20, None
                        text: "Please email nutribaseapp@gmail.com about any issues"

            AccordionItem:
                id: a3
                title: "Have an idea for the app"
                collapse: True
                background_normal: 'images/onpress.png'
                background_selected: 'images/pressed.png'
                BoxLayout:
                    orientation: "vertical"
                    padding_y: 15
                    Label:
                        color: "black"
                        text_size: root.width-20, None
                        text: "Please email nutribaseapp@gmail.com for recommendations"
            AccordionItem:
                id: a4
                title: "Deleted my account but I can still see the information."
                collapse: True
                background_normal: 'images/onpress.png'
                background_selected: 'images/pressed.png'
                color: 'black'
                BoxLayout:
                    orientation: "vertical"
                    padding_y: 15
                    Label:
                        color: "black"
                        text_size: self.size
                        text_size: root.width-20, None
                        text: "Once you log out you should not be able to log back in"

            AccordionItem:
                id: a5
                title: "I can't see my hospital's data"
                collapse: True
                background_normal: 'images/onpress.png'
                background_selected: 'images/pressed.png'
                color: 'black'
                BoxLayout:
                    orientation: "vertical"
                    padding_y: 15
                    Label:
                        color: "black"
                        text_size: self.size
                        text_size: root.width-20, None
                        text: "Make sure you are spelling your hospital name like your coworkers. If you need to change your hospital name click on profile from the main page. (Profile is found under the three vertical buttons at the top)"

            AccordionItem:
                id: a6
                title: "I want to update my email/official name/ title"
                collapse: True
                background_normal: 'images/onpress.png'
                background_selected: 'images/pressed.png'
                color: 'black'
                BoxLayout:
                    orientation: "vertical"
                    padding_y: 15
                    Label:
                        color: "black"
                        text_size: self.size
                        text_size: root.width-20, None
                        text: "Click on profile from the main page. (Profile is found under the three vertical buttons at the top)"

            AccordionItem:
                id: a7
                title: "Something you would like to see on the app"
                collapse: True
                background_normal: 'images/onpress.png'
                background_selected: 'images/pressed.png'
                BoxLayout:
                    orientation: "vertical"
                    padding_y: 15
                    Label:
                        color: "black"
                        text_size: root.width-20, None
                        text: "Please email nutribaseapp@gmail.com about any issues"


